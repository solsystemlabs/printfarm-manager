    "eslint-plugin-react-hooks": "^6.1.1",
    "jsdom": "^27.0.0",
    "postcss": "^8.5.1",
    "prettier": "^3.6.2",
    "prisma": "^6.17.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
