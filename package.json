    "@tanstack/react-router": "^1.132.33",
    "@tanstack/react-router-devtools": "^1.132.33",
    "@tanstack/react-router-ssr-query": "^1.132.33",
    "@tanstack/react-start": "^1.132.36",
    "pg": "^8.16.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "redaxios": "^0.5.1",
    "tailwind-merge": "^2.6.0"
